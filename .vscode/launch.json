    "version": "0.2.0",
    "configurations": [
        {
            "name": "mobile_app",
            "request": "launch",
            "type": "dart",
            "toolArgs": [
                "--dart-define", "SENTRY_DSN=${env:SENTRY_DSN}",
                "--dart-define", "SENTRY_ENVIRONMENT=${env:SENTRY_ENVIRONMENT}",
            ],
        },
        {
            "name": "mobile_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "toolArgs": [
                "--dart-define", "SENTRY_DSN=${env:SENTRY_DSN}",
                "--dart-define", "SENTRY_ENVIRONMENT=${env:SENTRY_ENVIRONMENT}",
            ],
        },
        {
            "name": "mobile_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define", "SENTRY_DSN=${env:SENTRY_DSN}",
                "--dart-define", "SENTRY_ENVIRONMENT=${env:SENTRY_ENVIRONMENT}",
            ],
        }
    ]
}